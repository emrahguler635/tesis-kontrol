{
  "version": 2,
  "builds": [
    {
      "src": "backend/server-postgres.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server-postgres.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/assets/$1"
    },
    {
      "src": "/logo.svg",
      "dest": "/public/logo.svg"
    },
    {
      "src": "/belediye.jpg",
      "dest": "/public/belediye.jpg"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "CACHE_BUST": "2025-08-03-21-50"
  },
  "alias": ["tesis-kontrol.vercel.app"]
} 